<?xml version="1.0" encoding="utf-8"?><androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/activity_main"    style="@style/an_match_match"    android:background="@color/anBackground">    <com.google.android.material.appbar.AppBarLayout        android:id="@+id/appbar"        style="@style/an_match_wrap"        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">        <com.google.android.material.appbar.CollapsingToolbarLayout            style="@style/an_match_wrap"            app:layout_scrollFlags="scroll|exitUntilCollapsed">            <ImageView                android:id="@+id/ivBackground"                style="@style/an_match_wrap"                android:scaleType="fitStart"                android:src="@drawable/bgwan" />            <!--标题 -->            <androidx.appcompat.widget.Toolbar                android:layout_width="match_parent"                android:layout_height="80dp"                app:contentInsetStart="0dp"                app:layout_collapseMode="pin">                <FrameLayout                    android:id="@+id/fl_layout"                    style="@style/an_match_match"                    android:background="@color/ColorChocolate"                    android:paddingTop="25dp"                    app:layout_collapseMode="pin">                    <FrameLayout                        style="@style/an_match_wrap"                        android:layout_margin="10dp"                        android:background="@android:color/white">                        <EditText                            android:id="@+id/editText"                            style="@style/an_match_match"                            android:background="@null"                            android:drawableLeft="@drawable/ic_color_share_web"                            android:hint="@string/app_name"                            android:padding="5dp"                            android:textColor="#999999"                            android:textColorHint="#999999" />                    </FrameLayout>                </FrameLayout>            </androidx.appcompat.widget.Toolbar>        </com.google.android.material.appbar.CollapsingToolbarLayout>    </com.google.android.material.appbar.AppBarLayout>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/recyclerview"        style="@style/an_match_match"        android:overScrollMode="never"        android:scrollbars="none"        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"        app:layout_behavior="@string/appbar_scrolling_view_behavior" /></androidx.coordinatorlayout.widget.CoordinatorLayout>