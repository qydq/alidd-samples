<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <!--根据enable测试结果-->    <item android:drawable="@color/ColorRed" android:state_pressed="true" /><!--ok保留===按下状态，如一个按钮触摸或者点击。-->    <item android:drawable="@color/btn_green_pressed" android:state_focused="true" /><!--取得焦点状态，比如用户选择了一个文本框。-->    <item android:drawable="@color/ColorBlueviolet" android:state_hovered="true" /><!--光标悬停状态，通常与focused state相同，它是4.0的新特性-->    <item android:drawable="@color/ColorBlack" android:state_selected="true" /><!--选中状态--><!--    <item android:drawable="@drawable/base_drawable_pressed" android:state_enabled="true" />&lt;!&ndash;ok保留===能够接受触摸或者点击事件XXX&ndash;&gt;-->    <item android:drawable="@drawable/ic_color_sel" android:state_checked="true" /><!--被checked了，如：一个RadioButton可以被check了。-->    <!--分割线-->    <item android:drawable="@color/picture_color_grey" android:state_enabled="false" /><!--ok保留===顺序5：能够接受触摸或者点击事件XXX-->    <item android:drawable="@drawable/in_drawable_ll_pressed" android:state_pressed="false" /><!--ok保留===顺序1： 按下状态，如一个按钮触摸或者点击。-->    <item android:drawable="@color/ColorGreenyellow" android:state_focused="false" /><!--顺序2：取得焦点状态，比如用户选择了一个文本框。-->    <item android:drawable="@color/an_color_holo_light_blue" android:state_hovered="false" /><!--顺序3：光标悬停状态，通常与focused state相同，它是4.0的新特性-->    <item android:drawable="@color/an_color_holo_deep_black" android:state_selected="false" /><!--顺序4：选中状态-->    <item android:drawable="@drawable/ic_color_sel_qq" android:state_checked="false" /><!--顺序6：被checked了，如：一个RadioButton可以被check了。--></selector>