<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    package="com.sunsty.alidd">    <!--备注一定要写好-->    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.RECORD_AUDIO" />    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" /> <!-- 应用的后台权限，运行在Android Q设备上,并且设置了target>28，必须增加这个权限声明才能录屏 -->    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />    <!--选择相册所需要的权限-->    <uses-permission android:name="android.permission.CAMERA" />    <uses-permission android:name="android.permission.WRITE_SETTINGS" />    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    <uses-permission android:name="android.permission.WRITE_MEDIA_STORAGE" />    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-feature        android:glEsVersion="0x00020000"        android:required="false" />    <uses-feature        android:name="android.software.live_wallpaper"        android:required="false" />    <application        android:name=".AliddApplication"        android:allowBackup="true"        android:icon="@drawable/ic_drawable_copy_fav"        android:label="@string/app_name"        android:roundIcon="@drawable/ic_drawable_copy_fav"        android:supportsRtl="true"        android:theme="@style/ParallaxAppTheme"        tools:replace="android:theme,android:name">        <provider            android:name="com.ali.module.lib.PictureFileProvider"            android:authorities="${applicationId}.provider"            android:exported="false"            android:grantUriPermissions="true"            tools:replace="name,authorities,exported,grantUriPermissions">            <meta-data                android:name="android.support.FILE_PROVIDER_PATHS"                android:resource="@xml/base_fileprovider_takephoto"                tools:replace="name,resource" />        </provider>        <!--file_paths-->        <activity android:name=".view.activity.MainActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".view.activity.VideoNoFFmpegActivity" />        <activity android:name=".view.activity.AnimationGifActivity" />        <activity android:name=".view.activity.BeforeSelectAlbumActivity" />        <activity android:name=".view.activity.EsayPermissionActivity" />        <activity android:name=".view.activity.ExpandableActivity" />        <activity android:name=".view.activity.DefaultRefActivity" />        <activity android:name=".view.activity.AliParallaxActivity" />        <activity android:name=".view.activity.HttpsRequestActivity" />        <activity android:name=".view.activity.HttpsDownloadActivity" />        <activity android:name=".view.activity.INATabLayoutActivity" />        <activity android:name=".view.activity.SwipeRecycleActivity" />        <activity android:name=".view.activity.StandardStoryActivity" />        <activity            android:name=".view.activity.TranslucentActivity"            android:theme="@style/TranslucentActivity" />        <!--** Xmediac属于FFmpeg C语言这一套、本项目集成视频模块由author : sunst提供并验证，        本人知乎会有原理使用介绍，如帮助到你请关注知乎https://zhihu.com/people/qydq-->        <!--        <activity                    android:name="com.sunsty.xmediac.FFmpegRenderActivity"                    android:screenOrientation="portrait">                    <intent-filter>                        <action android:name="abc" />                        <category android:name="android.intent.category.DEFAULT" />                    </intent-filter>                </activity>-->        <activity            android:name="com.sunsty.xmediac.FFmpegRenderActivity"            android:screenOrientation="portrait" />        <activity            android:name="com.sunsty.xmediac.sdl.SDLActivity"            android:configChanges="keyboardHidden|orientation|screenSize"            android:hardwareAccelerated="true" />        <!--录屏模块,需要放到xrecord 模块中、这是借助github silicompressorr实现。        本项目集成视频模块由sunst提供，如帮助到你请关注知乎https://zhihu.com/people/qydq-->        <activity            android:name="com.sunst.alidd.ScreenRecordActivity"            android:theme="@style/anTranslucentTheme" />        <!-- 全面屏-->        <meta-data            android:name="android.max_aspect"            android:value="2.1" />        <activity            android:name="com.sunsty.alidd.view.activity.picture.MainPictureActivity"            android:screenOrientation="portrait" />        <activity            android:name="com.sunsty.alidd.view.activity.picture.PhotoFragmentActivity"            android:configChanges="orientation|screenSize|keyboardHidden|keyboard"            android:screenOrientation="portrait" />        <activity            android:name="com.sunsty.alidd.view.activity.picture.SimpleActivity"            android:screenOrientation="portrait" />        <!--swiperecylcerview情景系列-->        <activity            android:name=".view.activity.swipe.menu.MenuActivity"            android:label="Item中的菜单" />        <activity            android:name=".view.activity.swipe.menu.ListActivity"            android:label="List形式菜单" />        <activity            android:name=".view.activity.swipe.menu.GridActivity"            android:label="Grid形式菜单" />        <activity            android:name=".view.activity.swipe.menu.ViewTypeActivity"            android:label="ViewType菜单" />        <activity            android:name=".view.activity.swipe.menu.VerticalActivity"            android:label="竖向排布的菜单" />        <activity            android:name=".view.activity.swipe.menu.DefineActivity"            android:label="自定义菜单" />        <!-- 小2部分 -->        <activity            android:name=".view.activity.swipe.move.MoveActivity"            android:label="Item拖拽和侧滑删除" />        <activity            android:name=".view.activity.swipe.move.DragSwipeListActivity"            android:label="ListItem拖拽/侧滑" />        <activity            android:name=".view.activity.swipe.move.DragGridActivity"            android:label="GridItem拖拽" />        <activity            android:name=".view.activity.swipe.move.DragTouchListActivity"            android:label="触摸即拖拽" />        <activity            android:name=".view.activity.swipe.move.DefineActivity"            android:label="自定义拖拽/侧滑" />        <!-- 小3部分：Header和Footer -->        <activity            android:name=".view.activity.swipe.header.HeaderViewActivity"            android:label="Header和Footer" />        <!-- 小4部分：刷新和加载更多 -->        <activity            android:name=".view.activity.swipe.load.RefreshLoadActivity"            android:label="刷新和加载更多" />        <activity            android:name=".view.activity.swipe.load.DefaultActivity"            android:label="默认LoadMore" />        <activity            android:name=".view.activity.swipe.load.DefineActivity"            android:label="自定义LoadMore" />        <!-- 小5部分：嵌套 -->        <activity            android:name=".view.activity.swipe.nested.NestedActivity"            android:label="滑动布局嵌套使用" />        <activity            android:name=".view.activity.swipe.nested.CardViewActivity"            android:label="CardView" />        <activity            android:name=".view.activity.swipe.nested.DrawerActivity"            android:label="DrawerLayout" />        <activity            android:name=".view.activity.swipe.nested.ViewPagerActivity"            android:label="ViewPager" />        <!-- 小6部分：二级列表 -->        <activity            android:name=".view.activity.swipe.expanded.ExpandableActivity"            android:label="二级列表" />        <activity            android:name=".view.activity.swipe.expanded.ListActivity"            android:label="List形式二级列表" />        <activity            android:name=".view.activity.swipe.expanded.GridActivity"            android:label="Grid形式二级列表" />        <activity            android:name=".view.activity.swipe.expanded.StaggeredActivity"            android:label="Staggered形式二级列表" />        <!-- 小7部分：黏贴分组 -->        <activity            android:name=".view.activity.swipe.group.GroupActivity"            android:label="Sticky分组效果" />        <activity            android:name=".view.activity.swipe.group.LayoutActivity"            android:label="普通布局中Sticky" />        <activity            android:name=".view.activity.swipe.group.MenuActivity"            android:label="Item菜单Sticky分组" />        <!--录屏service情景系列-->        <service            android:name="com.sunst.alidd.ScreenRecordService"            android:enabled="true"            android:foregroundServiceType="mediaProjection"            tools:targetApi="q" />    </application></manifest>