<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    package="com.sunsty.alidd">    <!--备注一定要写好-->    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.RECORD_AUDIO" />    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" /> <!-- 应用的后台权限，运行在Android Q设备上,并且设置了target>28，必须增加这个权限声明才能录屏 -->    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />    <uses-feature        android:glEsVersion="0x00020000"        android:required="false" />    <uses-feature        android:name="android.software.live_wallpaper"        android:required="false" />    <application        android:name=".AliddApplication"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/TranslucentActivity"        tools:replace="android:theme,android:name">        <activity            android:name=".view.activity.MainActivity"            android:theme="@style/TranslucentActivity_TEST">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".view.activity.VideoNoFFmpegActivity" />        <activity android:name=".view.activity.AnimationGifActivity" />        <activity android:name=".view.activity.BackMainActivity" />        <activity android:name=".view.activity.ExpandableActivity" />        <activity android:name=".view.activity.JustHttpsActivity" />        <activity android:name=".view.activity.INATabLayoutActivity" />        <activity android:name=".view.activity.MainActivityConfict" />        <activity            android:name=".view.activity.TranslucentActivity"            android:theme="@style/TranslucentActivity" />        <!--** Xmediac属于FFmpeg C语言这一套、本项目集成视频模块由author : sunst提供并验证，        本人知乎会有原理使用介绍，如帮助到你请关注知乎https://zhihu.com/people/qydq-->        <!--        <activity                    android:name="com.sunsty.xmediac.FFmpegRenderActivity"                    android:screenOrientation="portrait">                    <intent-filter>                        <action android:name="abc" />                        <category android:name="android.intent.category.DEFAULT" />                    </intent-filter>                </activity>-->        <activity            android:name="com.sunsty.xmediac.FFmpegRenderActivity"            android:screenOrientation="portrait"></activity>        <activity            android:name="com.sunsty.xmediac.sdl.SDLActivity"            android:configChanges="keyboardHidden|orientation|screenSize"            android:hardwareAccelerated="true" />        <!--录屏模块,需要放到xrecord 模块中、这是借助github silicompressorr实现。        本项目集成视频模块由sunst提供，如帮助到你请关注知乎https://zhihu.com/people/qydq-->        <activity            android:name="com.sunst.alidd.ScreenRecordActivity"            android:theme="@style/anTranslucentTheme" />        <service            android:name="com.sunst.alidd.ScreenRecordService"            android:enabled="true"            android:foregroundServiceType="mediaProjection"            tools:targetApi="q" />    </application></manifest>