<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:drawable="@color/ColorWhite" />    <item android:drawable="@drawable/tab_selected" android:state_pressed="true" /><!--按下状态，如一个按钮触摸或者点击。-->    <item android:drawable="@color/btn_green_pressed" android:state_focused="true" /><!--取得焦点状态，比如用户选择了一个文本框。-->    <item android:drawable="@color/ColorBlueviolet" android:state_hovered="true" /><!--光标悬停状态，通常与focused state相同，它是4.0的新特性-->    <item android:drawable="@color/ColorBlack" android:state_selected="true" /><!--选中状态-->    <item android:drawable="@drawable/base_drawable_check_on" android:state_enabled="true" /><!--能够接受触摸或者点击事件XXX-->    <item android:drawable="@drawable/ic_picture_sel" android:state_checked="true" /><!--被checked了，如：一个RadioButton可以被check了。-->    <item android:drawable="@drawable/in_drawable_ll_pressed" android:state_pressed="false" /><!--按下状态，如一个按钮触摸或者点击。-->    <item android:drawable="@color/ColorGreenyellow" android:state_focused="false" /><!--取得焦点状态，比如用户选择了一个文本框。-->    <item android:drawable="@color/an_color_holo_light_blue" android:state_hovered="false" /><!--光标悬停状态，通常与focused state相同，它是4.0的新特性-->    <item android:drawable="@color/an_color_holo_deep_black" android:state_selected="false" /><!--选中状态-->    <item android:drawable="@drawable/base_drawable_check_off" android:state_enabled="false" /><!--能够接受触摸或者点击事件XXX-->    <item android:drawable="@drawable/ic_picture_sel_qq" android:state_checked="false" /><!--被checked了，如：一个RadioButton可以被check了。--></selector>